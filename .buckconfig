[repositories]
root = .
prelude = prelude
shim = shim

[repository_aliases]
ovr_config = prelude
toolchains = shim
fbcode = shim
fbcode_macros = shim
fbsource = shim
buck = shim

[buildfile]
name = TARGETS

[build]
execution_platforms = ovr_config//platforms:default

[parser]
target_platform_detector_spec = target:root//...->ovr_config//platforms:default target:shim//...->ovr_config//platforms:default
