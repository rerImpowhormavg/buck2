[repositories]
self = .
prelude = prelude
toolchains = toolchains
none = none

[repository_aliases]
ovr_config = prelude
buck = none
fbcode = none
fbcode_macros = none
fbsource = none

[buildfile]
name = BUILD

[build]
execution_platforms = ovr_config//platforms:default

[parser]
target_platform_detector_spec = target://...->ovr_config//platforms:default
